{
    "model": {
        "type": "minecraft:select",
        "property": "minecraft:custom_model_data",
        "cases": [
            {
                "when": "minecraft:overworld",
                "model": {
                    "type": "minecraft:model",
                    "model": "improved-maps:item/atlas_overworld"
                }
            },
            {
                "when": "minecraft:the_nether",
                "model": {
                    "type": "minecraft:model",
                    "model": "improved-maps:item/atlas_nether"
                }
            },
            {
                "when": "minecraft:the_end",
                "model": {
                    "type": "minecraft:model",
                    "model": "improved-maps:item/atlas_end"
                }
            }
        ],
        "fallback": {
            "type": "minecraft:model",
            "model": "improved-maps:item/atlas_unknown"
        }
    },
    "hand_animation_on_swap": false
}